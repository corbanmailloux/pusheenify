{
    "name": "Pusheenify",
    "version": "1.0",
    "description": "Because you can never have too much Pusheen.",
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "icons": {
        "128": "logo-128.png"
    },

    "browser_action": {
        "default_icon": "logo.png"
    },

    "background": {
        "scripts": [
            "background.js"
        ]
    },

    "manifest_version": 2
}
